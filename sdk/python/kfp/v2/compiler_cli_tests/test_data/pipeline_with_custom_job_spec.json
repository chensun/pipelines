{
  "pipelineSpec": {
    "components": {
      "comp-hello-world-op": {
        "executorLabel": "exec-hello-world-op",
        "inputDefinitions": {
          "parameters": {
            "input1": {
              "type": "STRING"
            }
          }
        },
        "outputDefinitions": {
          "artifacts": {
            "output1": {
              "artifactType": {
                "schemaTitle": "system.Artifact"
              }
            }
          }
        }
      }
    },
    "deploymentSpec": {
      "executors": {
        "exec-hello-world-op": {
          "customJob": {
            "customJob": {
              "jobSpec": {
                "workerPoolSpecs": [
                  {
                    "containerSpec": {
                      "command": [
                        "sh",
                        "-exc",
                        "echo \"$0\" | gsutil cp - \"$1\"",
                        "{{$.inputs.parameters['input1']}}",
                        "{{$.outputs.artifacts['output1'].uri}}"
                      ],
                      "imageUri": "google/cloud-sdk:slim"
                    },
                    "machineSpec": {
                      "machineType": "n1-standard-4"
                    },
                    "replicaCount": "1"
                  }
                ]
              }
            }
          }
        }
      }
    },
    "pipelineInfo": {
      "name": "pipeline-with-custom-job-spec"
    },
    "root": {
      "dag": {
        "tasks": {
          "task-hello-world-op": {
            "componentRef": {
              "name": "comp-hello-world-op"
            },
            "inputs": {
              "parameters": {
                "input1": {
                  "componentInputParameter": "text"
                }
              }
            },
            "taskInfo": {
              "name": "task-hello-world-op"
            }
          }
        }
      },
      "inputDefinitions": {
        "parameters": {
          "text": {
            "type": "STRING"
          }
        }
      }
    },
    "schemaVersion": "2.0.0",
    "sdkVersion": "kfp-1.5.0"
  },
  "runtimeConfig": {
    "gcsOutputDirectory": "dummy_root",
    "parameters": {
      "text": {
        "stringValue": "hello world"
      }
    }
  }
}